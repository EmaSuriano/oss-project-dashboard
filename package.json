    "not op_mini all"
  ],
  "homepage": "https://oss-dashboard.netlify.com/",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.4",
    "apollo-boost": "^0.4.7",
    "argon-dashboard-react": "1.0.0",
    "babel-loader": "8.0.5",
    "enhanced-resolve": "^5.0.0-beta.4",
    "graphql": "^14.6.0",
    "patch-package": "^6.2.2",
    "query-string": "^6.12.1",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "@percy/cypress": "^2.3.1",
