{
  "name": "oss-project-dashboard-react",
  "version": "1.0.0",
  "description": "Argon Dashboard to show the status of github projects",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/argon-dashboard-react.git"
  },
  "keywords": [
    "github",
    "open source",
    "dashboard",
    "dashboard-react",
    "argon-dashboard",
    "argon-dashboard-react"
  ],
  "author": "Ema Suriano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmaSuriano/oss-project-dashboard/issues"
  },
  "scripts": {
    "prestart": "ts-node --skip-project generate-validators.ts src/types",
    "start": "react-scripts start",
    "prebuild": "ts-node --skip-project generate-validators.ts src/types",
    "build": "CI=true react-scripts build",
    "test": "percy exec -t 350 -- cypress run",
    "test:open": "percy exec -t 350 -- cypress open",
    "test:ci": "start-server-and-test 3000"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": "https://oss-dashboard.netlify.com/",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "graphql": "^14.7.0",
    "grommet": "^2.15.0",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-gist": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "rsuite": "^4.8.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@percy/cypress": "^2.3.1",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "cypress": "^4.12.1",
    "start-server-and-test": "^1.11.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "typescript-json-validator": "^2.4.2"
  }
}
